void DrawIt(TssStruct *tss)
{
  I8 *r;
  GrBitMap *base=GrAlias(grbase2,tss);

//The following draws the normal text.
  LtfUpdateBothLtf(tss);

  base->flags|=BMF_TRANSFORMATION;
  Free(base->r,tss);
  r=GrTheta(0.25*Sin(pi2*t),tss);
  base->r=GrScaleMat(r,1.5+0.5*Sin(t),tss);
  Free(r,tss);
  base->x=x;
  base->y=y;
  base->z=0;
  if (t%0.5>0.25)
    GrElemsPlot(base,0,0,0,__BIN_1);
  else
    GrElemsPlot(base,0,0,0,__BIN_2);

  GrDel(base);
}

