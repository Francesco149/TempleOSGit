I8 x=0,y=20;

void AnimateTsk()
{
  while (TRUE) {
    x++;
    if (x>Fs->parent_tss->win_pixel_right-
	  Fs->parent_tss->win_pixel_left)
      x=0;
    Sleep(25);
  }
}


//When using flood-fill in a graphic
