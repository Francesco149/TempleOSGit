	MOV	U4 FS:[TSS_ESP],EBP

	MOV	ESI,U4 FS:[TSS_NEXT_TSS]
	MOV	AX,U2 TSS_FS[ESI]
	MOV	FS,AX
	JMP	U4 SWAP_IN_NEXT_PART2

