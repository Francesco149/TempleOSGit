COUT_JOIN::
	OR	EBX,EBX
	JZ	U4 @@100
@@20:	MOV	EDX,U4 [EBP]
	MOV	EAX,U4 8[EBP]
	PUSHAD

	CMP	EDX,IT_NUM_IT+IT_I1
	JNE	@@25
	PUSH	ESI
	MOV	ESI,EAX
	CALL	PUT_STRING
	POP	ESI
	JMP	@@29
@@25:	CMP	EDX,IT_U8
	JE	@@26
	CMP	EDX,IT_I8
	JNE	@@24
@@26:	MOV	EAX,U4 12[EBP]
	CALL	PUT_HEX
	MOV	EAX,U4 8[EBP]
	CALL	PUT_HEX
	JMP	@@29
@@24:	CMP	EDX,IT_U4
	JE	@@21
	CMP	EDX,IT_I4
	JNE	@@21A
@@21:	CALL	PUT_HEX
	JMP	@@29
@@21A:	CMP	EDX,IT_U2
	JE	@@22
	CMP	EDX,IT_I2
	JNE	@@22A
@@22:	CALL	PUT_HEX_U2
	JMP	@@29
@@22A:	CMP	EDX,IT_U1
	JNE	@@23
	CALL	PUT_HEX_U1
	JMP	@@29
@@23:	CMP	EDX,IT_DOUBLE
	JNE	@@30
	PUSH	U4 12[EBP]
	PUSH	U4 8[EBP]
	CALL	CP_PUT_FLOAT+4
	ADD	ESP,8
	JMP	@@29
@@30:	CMP	EDX,IT_I1
	JNE	@@21
	CALL	PUT_I1

@@29:	POPAD
	SUB	EBP,16
	DEC	EBX
	JNZ	U4 @@20
@@100:	RET

	ALIGN	8,0x90
