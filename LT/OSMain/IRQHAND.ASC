	PUSHAD
	MOV	ESI,MSG_GETCHAR
	CALL	FIND_EXTERN
	JNZ	@@3210
	CALL	GET_KEY
	INT	I_PUT_I1
	XOR	EDX,EDX
	JMP	@@3220
@@3210:	PUSH	0
	PUSH	0
	CALL	ESI
	ADD	ESP,8
@@3220:	MOV	EBP,ESP
	MOV	U4 SP_EAX[EBP],EAX
