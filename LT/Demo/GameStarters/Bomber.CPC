#define NUM_TREES	128
class Tree
{
  I64 x,y;
} tr[NUM_TREES];
 
#define NUM_TANKS	64
class Tank
{
  F64 x,y,theta;
  BoolI8 dead,pad[7];
} tk[NUM_TANKS];
 
F64 v,x,y,theta,thetaf;
F64 bt,bx,by;
 
U0 UpdateWin(TaskStruct *task)
{
  GrDC *dc=GrDCAlias(gr_dc,task);
  I64 i;
  for (i=0;i<NUM_TANKS;i++)
    if (tk[i].dead) {
      if (i&1) {
	dc->flags|=DCF_SYMMETRY|DCF_JUST_MIRROR;
	GrSetSymmetry(dc,tk[i].x,tk[i].y,tk[i].x+1024*Cos(tk[i].theta),tk[i].y-1024*Sin(tk[i].theta));
      }
      if (Blink(15,tP(task)))
	GrSpritePlotRotZ3b(dc,tk[i].x,tk[i].y,0,__BIN_4,tk[i].theta);
      else
	GrSpritePlotRotZ3b(dc,tk[i].x,tk[i].y,0,__BIN_5,tk[i].theta);
      dc->flags&=~(DCF_SYMMETRY|DCF_JUST_MIRROR);
    } else
      GrSpritePlotRotZ3b(dc,tk[i].x,tk[i].y,0,__BIN_3,tk[i].theta);
  for (i=0;i<NUM_TREES;i++)
    GrSpritePlot3(dc,tr[i].x,tr[i].y,0,__BIN_2);
  if (bt) {
    if (bt+.25<tP(task))
      bt=0;
    else {
      if (Blink(10,tP(task)))
	GrSpritePlot3(dc,bx,by,0,__BIN_6);
      else
	GrSpritePlot3(dc,bx,by,0,__BIN_7);
    }
  }
  GrSpritePlotRotZ3b(dc,x,y,0,__BIN_1,theta);
}
 
I64 AnimateTask(TaskStruct *parent)
{ //Starts with preempt OFF
  I64 w=parent->win_pixel_width,
      h=parent->win_pixel_height;
  while (TRUE) {
    Snd(60+0.001*(1.0+Abs(Unwrap(thetaf-theta,-pi)))`8.0);
    if (x+parent->win_scroll_x<0.25*w)
      parent->win_scroll_x+=3;
    if (x+parent->win_scroll_x>0.75*w)
      parent->win_scroll_x-=3;
    if (y+parent->win_scroll_y<0.25*h)
      parent->win_scroll_y+=3;
    if (y+parent->win_scroll_y>0.75*h)
      parent->win_scroll_y-=3;
    theta+=.01*(thetaf-theta);
    x+=v*Cos(theta);
    y+=v*Sin(theta);
    Sleep(10);
  }
  return 0;
}
 
U0 Init()
{
  I64 i;
  bt=0;
  v=0.2;
  x=Fs->win_pixel_width/2;
  y=Fs->win_pixel_height/2;
  thetaf=theta=0;

  MemSet(tr,0,sizeof(tr));
  for (i=0;i<NUM_TREES;i++) {
    tr[i].x=RandU32%(GR_WIDTH*3)-GR_WIDTH;
    tr[i].y=RandU32%(GR_HEIGHT*3)-GR_HEIGHT;
  }

  MemSet(tk,0,sizeof(tk));
  for (i=0;i<NUM_TANKS;i++) {
    tk[i].x=RandU32%(GR_WIDTH*3)-GR_WIDTH;
    tk[i].y=RandU32%(GR_HEIGHT*3)-GR_HEIGHT;
    tk[i].theta=Rand*2*pi;
  }
}

U0 BombDrop()
{
  I64 i;
  bx=x;
  by=y;
  bt=tP(Fs);
  Noise(200,500,1000);
  for (i=0;i<NUM_TANKS;i++) {
    if (!tk[i].dead) {
      if (SqrI64(bx-tk[i].x)+SqrI64(by-tk[i].y)<20*20)
	tk[i].dead=TRUE;
    }
  }
}
 
U0 Bomber()
{
  U64 ch,sc;
  MenuPush(
  "File {"
  "  Abort(,CH_SHIFT_ESC);"
  "  Exit(,CH_ESC);"
  "}"
  "Play {"
  "  Restart(,CH_CR);"
  "  Faster(,,SC_CURSOR_UP);"
  "  Slower(,,SC_CURSOR_DOWN);"
  "  Left(,,SC_CURSOR_LEFT);"
  "  Right(,,SC_CURSOR_RIGHT);"
  "  Bomb(,CH_SPACE);"
      "}"
      );
  SettingsPush; //See [C:/LT/Adam/TaskSettings.APZ,3] SettingsPush
  WordStat(OFF);
  WinMax;
  Preempt(OFF);
  Fs->animate_task=Spawn(&AnimateTask,Fs,"Animate",Fs);
  Fs->update_win=&UpdateWin;
  Init;
  while (TRUE) {
    ch=GetKey(&sc);
    switch (ch) {
      case 0:
	switch (sc.u8[0]) {
	   case SC_CURSOR_UP:
	     v+=0.1;
	     if (v>3.0) v=3.0;
	     break;
	   case SC_CURSOR_DOWN:
	     v-=0.1;
	     if (v<0.2) v=0.2;
	     break;
	   case SC_CURSOR_LEFT:
	     thetaf-=0.5;
	     break;
	   case SC_CURSOR_RIGHT:
	     thetaf+=0.5;
	     break;
 
	}
	break;
      case CH_SPACE:
	BombDrop;
	break;
      case CH_CR:
	Init;
	break;
      case CH_ESC:
      case CH_SHIFT_ESC:
	goto bm_done;
    }
  }
bm_done:
  SettingsPop;
  MenuPop;
}
 
Bomber;
 
