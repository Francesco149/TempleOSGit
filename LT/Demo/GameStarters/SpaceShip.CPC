F64 enemy_destroyed_time=0,
   player_destroyed_time=0,
   phasor_time=0;
I64 x=100,y=100;
GrDC *dc=GrDCAlias(gr_dc,Fs);

#define NUM_STARS	100
I64 stars_x[NUM_STARS],stars_y[NUM_STARS];

U0 PhasorTask(U64 dummy=0)
{
  nounusedwarn dummy;
  I64 i;
  for (i=2000;i>100;i-=6) {
    Snd(i);
    Sleep(2);
  }
  Snd(0);
}

U0 UpdateWin(TaskStruct *task)
{
  nounusedwarn task;
  U64 i;
  I64 xx=ipx-task->win_pixel_left-task->win_scroll_x,
     yy=ipy-task->win_pixel_top-task->win_scroll_y;
 
  dc->color=WHITE;
  for (i=0;i<NUM_STARS;i++)
    GrPlot(dc,stars_x[i],stars_y[i]);

  dc->bkcolor=BLACK;
  if (enemy_destroyed_time) {
    GrSpritePlot3(dc,x,y,0,__BIN_3);
    if (tP(task)>=enemy_destroyed_time)
      enemy_destroyed_time=0;
  } else
    GrSpritePlot3(dc,x,y,0,__BIN_1);

  if (!player_destroyed_time) {
    dc->collision_cnt=0;
    dc->bkcolor=ROP_COLLISION+BLACK;
    GrSpritePlot3(dc,xx,yy,0,__BIN_2);
    if (dc->collision_cnt>0x20) {
      player_destroyed_time=tP(task)+1.0;  //one second
      Noise(700,150,200);
    }
  }
  dc->bkcolor=BLACK;
  if (player_destroyed_time) {
    GrSpritePlot3(dc,xx,yy,0,__BIN_3);
    if (tP(task)>=player_destroyed_time)
      player_destroyed_time=0;
  } else
    GrSpritePlot3(dc,xx,yy,0,__BIN_2);

  if (phasor_time) {
    dc->collision_cnt=0;
    dc->color=ROP_COLLISION;
    dc->bkcolor=ROP_COLLISION+BLACK;
    GrLine(dc,xx+45,yy,xx+300,yy);
    if (!enemy_destroyed_time && dc->collision_cnt>8){
      enemy_destroyed_time=tP(task)+1.0;	//one second
      Noise(700,150,200);
    }

    dc->color=BLUE;
    GrLine(dc,xx+45,yy,xx+300,yy);
    if (tP(task)>=phasor_time)
      phasor_time=0;
  }

}


U0 SpaceShip()
{
  U64 i;
  for (i=0;i<NUM_STARS;i++) {
    stars_x[i]=RandU16%GR_WIDTH;
    stars_y[i]=RandU16%GR_HEIGHT;
  }
  SettingsPush; //See [C:/LT/Adam/TaskSettings.APZ,3] SettingsPush
  WinMax;
  Preempt(OFF);
  Fs->win_inhibit=WIF_ALL-WIF_BORDER;
  WinBorder(OFF);
  WordStat(OFF);
  LtfClear;

  //This is needed for an obscure reason.  We check
  //for collisions inside the UpdateWin by counting non-black
  //pixels.  The screen update is such that the text
  //layer lags the graphic layer by one refresh.
  //Normally, it's not a problem.
  Fs->text_attr=BLACK+BLACK<<4;
  WinTextClear; WinSync;

  Fs->update_win=&UpdateWin;
  try {
    while (!ScanChar) {
      x-=3; y-=2;
      if (x<0) x=GR_WIDTH;
      if (y<0) y=GR_HEIGHT;
      if (ip_lb) {  //left button?
	phasor_time=tP+0.5;  //half second
	Spawn(&PhasorTask);
      }
      Sleep(50);
    }
  } catch
    CatchAll;
  SettingsPop;
}

SpaceShip;
