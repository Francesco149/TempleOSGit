		break;
	    case ROPB_XOR:
	      if (win_z_buf_ptr)
		for (j=h1;j<h2;j++) {
		  if (Bt(&color,p1)) {
		    color_byte=0xAA;
		    dst=dc->body+kk1>>3;
		    if (leading_bits) {
		      if (win_z_num>=*win_z_buf_ptr++)
			*dst=*dst&leading_bit_mask^color_byte&~leading_bit_mask;
