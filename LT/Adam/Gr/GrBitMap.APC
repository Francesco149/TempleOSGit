	    case ROPB_XOR:
	      if (mask_dst)
		mask_dst[dst]|=bit;
	      if (c & 1) *dst^=bit;
	      dst+=d;
	      if (c & 2) *dst^=bit;
	      dst+=d;
	      if (c & 4) *dst^=bit;
	      dst+=d;
	      if (c & 8) *dst^=bit;
	      break;
	  }
	  result++;
	  x--;
	} else
	  break;
      }
      return result;
    case DCT_MONO:
      if (mask=dc->mask)
	if (mask->alias)
	  mask->alias->flags|=DCF_DIRTY;
      while (TRUE) {
	x3=x;
	if (!(x3&(FONT_WIDTH-1))) {
