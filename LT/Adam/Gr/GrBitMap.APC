		    if (trailing_bits && win_z_num>=*win_z_buf_ptr++)
		      *dst=*dst&trailing_bit_mask^color_byte&~trailing_bit_mask;
		  } else
		    win_z_buf_ptr><(U8 *)+=win_z_buf_line_dec;
		  if ((j+y)&7==7)
		    win_z_buf_ptr><(U8 *)+=win_z_buf_line_inc;
		  else
		    win_z_buf_ptr><(U8 *)-=win_z_buf_line_dec;
