void Test()
{
  U1 *img1=__BIN_1;
  U1 *img2=__BIN_2;
  U1 *img3=__BIN_3;
  GrBitMap *base=GrNew(grbase);
  I4 i=0;

  while (i<100) {
    GrClear;
    base->color=GR_RED;
    base->pen_width=3;
    GrElemsPlot(base,i&511,i&255,0,img1,GR_WHITE);
    GrElemsPlot(base,i&127,i&511,0,img2,GR_WHITE);
    GrElemsPlot(base,i&127,i&511,0,img3,GR_WHITE);
    i++;
    Sleep(10);
  }
}
