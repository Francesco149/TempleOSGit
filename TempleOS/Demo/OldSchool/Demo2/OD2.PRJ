#include "::/TempleOS/Kernel/AOTAdam.HPP"

asm {
USE64
	IMPORT	Beep;
_MY_BEEP::
	ENTER	0

	PUSH	FALSE
	PUSH	300
	CALL	Beep

	LEAVE
	RET
}

_extern _MY_BEEP U0 MyBeep();

U0 Routine2(U8 *st)
{
  Print("Routine2:%s\n",st);
  MyBeep();
}

