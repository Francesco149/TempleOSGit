// imports all syms from the Adam Task's hash table.
#include "::/Kernel/AOT.HPP"

asm {
USE64
	IMPORT	Beep;
_MY_BEEP::
	PUSH	RBP
	MOV	RBP,RSP

	PUSH	FALSE
	PUSH	300
	CALL	Beep

	POP	RBP
	RET
}

_extern _MY_BEEP U0 MyBeep();

U0 Routine2(U8 *st)
{
  Print("Routine2:%s\n",st);
  MyBeep();
}

Print("$BK,1$Woo-Hoo!$BK,0$\n");
Dir;
Print("$BK,1$Woo-Hoo!$BK,0$\n");

Routine2("Cool");

